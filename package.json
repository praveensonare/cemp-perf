  "scripts": {
    "start": "react-scripts start",
    "json-server": "json-server --watch src/database/db.json --host 127.0.0.1 --port 3004",
    "cf-deploy": "aws s3 sync  build s3://smart-energy-website-hosting-2",
    "cf-invalidate": "aws cloudfront create-invalidation --distribution-id EPVYLEZ2DC7G2 --paths '/*'",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test:failed": "react-scripts test -- --onlyFailures",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.8",
    "@testing-library/react": "^14.1.2",
    "axios-mock-adapter": "^1.22.0",
    "babel-jest": "^29.7.0",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!(@aws-amplify/api-rest|axios|react-bootstrap)).+\\.js$"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/appStore.jsx",
      "!src/app/store.js",
      "!src/reportWebVitals.js",
      "!src/index.js",
      "!src/pages/Contact.js",
      "!src/**/__tests__/**/*",
      "!src/**/*.{test,spec}.js",
      "!src/pages/ProjectSchematic/**/*",
      "!src/pages/RoleManager/GuidedTourRoleManager.js",
      "!src/contexts/WebSocketContext.js"
    ]
