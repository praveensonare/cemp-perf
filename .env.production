# Production environment variables for performance optimization

# Disable source maps in production for smaller bundle size and security
GENERATE_SOURCEMAP=false

# Enable production optimizations
INLINE_RUNTIME_CHUNK=false

# Reduce console logs in production (optional)
REACT_APP_ENV=production

# Enable code splitting and tree shaking (default in CRA but explicit here)
EXTEND_ESLINT=true

# Image optimization settings (for improved performance)
IMAGE_INLINE_SIZE_LIMIT=10000

# Optimize CSS extraction
DISABLE_ESLINT_PLUGIN=false
